{
	"plugins": [
		"transform-async-to-generator", ["transform-es2015-modules-commonjs", {
			"allowTopLevelThis": true
		}],
		["transform-runtime", {
			"polyfill": true,
			"regenerator": true
		}],
		["transform-object-rest-spread", { "useBuiltIns": true }]
	],
	"env": {
		"production": {
			"plugins": ["transform-remove-console"]
		}
	}
}